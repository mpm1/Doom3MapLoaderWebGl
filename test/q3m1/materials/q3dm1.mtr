{
	qer_editorimage textures/base_wall/blink_quake.tga 

	DECAL_MACRO
	noShadows
	nonsolid
	noimpact
	translucent

	{       
     	 blend	diffuseMap  //GL_SRC_ALPHA, GL_ONE_MINUS_SRC_ALPHA
	 map	textures/base_wall/blink_quake_d.tga 

	 //alphaTest 0.5
	}

{
		blend 	add
		map	textures/base_wall/blink_quake.tga
		 	rgb (.5 * sintable [.002 + time ]) + .2
	}


}
