textures/sfx/center_fire
{
	qer_editorimage 		textures/sfx/center_fire_d.tga
	{
	blend 			diffusemap	
	map 				textures/sfx/center_fire_d.tga
	}
	{
	maskcolor			
	map 				textures/sfx/center_fire_mask.tga
	}
	{
	blend  			gl_dst_alpha, gl_one_minus_dst_alpha
       rotate time * 0.3
centerScale 1, 1
	map				textures/sfx/fire_effect_sfx_3.tga
	}      
}
textures/sfx/firedoor
{
	qer_editorimage 		textures/sfx/firedoor_d.tga
	{
	blend 			diffusemap	
	map 				textures/sfx/firedoor_d.tga
	}
	{
	maskcolor			
	map 				textures/sfx/firedoor_mask.tga
	}
	{
	blend  			gl_dst_alpha, gl_one_minus_dst_alpha
	translate			time * 2, 2
       rotate time*0.4
scale		1.5, 1.5

	map				textures/sfx/fire_effect_sfx_2.tga
	}      
}
